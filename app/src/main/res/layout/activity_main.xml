    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/title_text"
        android:text="@string/title"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:id="@+id/swedish_flag"
        android:src="@drawable/svensk_flagga_2"
        android:background="@android:color/transparent"
        android:layout_below="@id/title_text"
        />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:id="@+id/brittish_flag"
        android:layout_below="@id/title_text"
        android:src="@drawable/brittish_flag_2"
        android:background="@android:color/transparent"
        android:layout_toRightOf="@id/swedish_flag"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:id="@+id/launch_button"
        android:src="@drawable/squirly_arrow"
        android:background="@android:color/transparent"
        android:layout_below="@id/swedish_flag"/>
